{
  "_version": "1.7.0",
  "sap.app": {
    "id": "com.ui5.dotproject.timecard",
    "type": "application",
    "i18n": "i18n/i18n.properties",
    "applicationVersion": {
      "version": "1.0.0"
    },
    "title": "{{appTitle}}",
    "description": "{{appDescription}}",
    "sourceTemplate": {
      "id": "ui5template.basicSAPUI5ApplicationProject",
      "version": "1.40.12"
    }
  },
  "sap.ui": {
    "technology": "UI5",
    "icons": {
      "icon": "",
      "favIcon": "",
      "phone": "",
      "phone@2": "",
      "tablet": "",
      "tablet@2": ""
    },
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    },
    "supportedThemes": ["sap_hcb", "sap_belize"]
  },
  "sap.ui5": {
    "handleValidation": true,
    "rootView": {
      "viewName": "com.ui5.dotproject.timecard.view.App",
      "type": "XML"
    },
    "dependencies": {
      "minUI5Version": "1.30.0",
      "libs": {
        "sap.ui.core": {},
        "sap.m": {},
        "sap.ui.layout": {}
      }
    },
    "contentDensities": {
      "compact": true,
      "cozy": true
    },
    "models": {
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleName": "com.ui5.dotproject.timecard.i18n.i18n"
        }
      }
    },
    "resources": {
      "css": [{
        "uri": "css/style.css"
      }]
    },
    "routing": {
      "targets": {
        "settings": {
          "viewType": "XML",
          "transition": "slide",
          "clearAggregation": false,
          "viewName": "Settings",
          "title": "",
          "controlId": "idApp",
          "controlAggregation": "pages",
          "viewPath": "com.ui5.dotproject.timecard.view"
        },
        "main": {
          "viewType": "XML",
          "transition": "slide",
          "clearAggregation": true,
          "viewName": "Main",
          "controlId": "idApp",
          "controlAggregation": "pages",
          "title": "",
          "viewPath": "com.ui5.dotproject.timecard.view"
        }
      },
      "routes": [{
        "name": "main",
        "pattern": "",
        "titleTarget": "",
        "greedy": false,
        "target": ["main"]
      }, {
        "name": "settings",
        "pattern": "settings",
        "titleTarget": "",
        "greedy": false,
        "target": ["settings"]
      }],
      "config": {
      	"routerClass": "sap.m.routing.Router",
        "viewPath": "com.ui5.dotproject.timecard.view",
        "controlId": "idApp",
        "controlAggregation": "pages",
        "viewLevel": 0,
        "clearAggregation": false
      }
    }
  }
}